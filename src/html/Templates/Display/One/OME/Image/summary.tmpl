<TABLE width="100%">
<TR>
	<TD COLSPAN="2">
		<table width="100%"><tr><td>
		<a title="<TMPL_VAR NAME=name>" href="<TMPL_VAR NAME=/obj_detail_url>" class="ome_detail">
			<TMPL_VAR NAME=name/max_text_length-25>
		</a>
		</td><td align="right">
		<TMPL_VAR NAME=/selector>
		</td></tr></table>
	</TD>
</TR>
<TR>
	<TD WIDTH="50">
		<img src="<TMPL_VAR NAME=thumb_url>" Usemap="#Image<TMPL_VAR NAME=id>">
		<map name="Image<TMPL_VAR NAME=id>">
			<area shape="rect" coords="0,0,25,25" Title="Info for <TMPL_VAR NAME=name>" href="<TMPL_VAR NAME='/obj_detail_url'>">
			<area shape="rect" coords="0,0,50,50" Title="View <TMPL_VAR NAME=name>" href="javascript: openPopUpImage(<TMPL_VAR NAME=id>);">
		</map>
	</TD>
	<TD ALIGN="left">
		<TMPL_VAR NAME=owner>
		<BR>
		<TMPL_VAR NAME=group>
		<BR>
		<TMPL_VAR NAME=created>
	</TD>
</TR>
<TR>
	<TD COLSPAN="2">
		<TMPL_VAR NAME=description/max_text_length-60>
		<BR>
		<TMPL_VAR NAME=module_executions/render-popup>
	</TD>
</TR>
</TABLE>
