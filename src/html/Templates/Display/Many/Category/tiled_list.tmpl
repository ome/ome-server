<TMPL_IF NAME=/tile_loop/width-3>
	<TMPL_VAR NAME=/pager_text>
	<TMPL_IF NAME=/more_info_url>
		<TMPL_IF NAME=/pager_text>|</TMPL_IF>
		<a href="<TMPL_VAR NAME=/more_info_url>">More info on these Categories...</a>
	</TMPL_IF>
	<span class='ome_quiet'>Click the upper half of a thumbnail to see image info,
	click the lower half to open the image viewer.</span>
	<table class="ome_table">
		<TMPL_LOOP NAME=/tile_loop/width-3>
			<tr>
				<TMPL_LOOP NAME=/obj_loop>
					<td class="ome_td" valign="top"><TMPL_VAR NAME=/object/render-summary_no_thumbnail_controls></td>
				</TMPL_LOOP>
			</tr>
		</TMPL_LOOP>
	</table>
<TMPL_ELSE>
<b>(none)</b>
</TMPL_IF>
