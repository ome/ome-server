<OME xmlns="http://www.openmicroscopy.org/XMLschemas/OME/FC/ome.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:AML="http://www.openmicroscopy.org/XMLschemas/AnalysisModule/RC1/AnalysisModule.xsd" xmlns:STD="http://www.openmicroscopy.org/XMLschemas/STD/RC2/STD.xsd" xsi:schemaLocation="http://www.openmicroscopy.org/XMLschemas/OME/FC/ome.xsd http://www.openmicroscopy.org/XMLschemas/OME/FC/ome.xsd http://www.openmicroscopy.org/XMLschemas/AnalysisModule/RC1/AnalysisModule.xsd http://www.openmicroscopy.org/XMLschemas/AnalysisModule/RC1/AnalysisModule.xsd http://www.openmicroscopy.org/XMLschemas/STD/RC2/STD.xsd http://www.openmicroscopy.org/XMLschemas/STD/RC2/STD.xsd">      
	<SemanticTypeDefinitions xmlns="http://www.openmicroscopy.org/XMLschemas/STD/RC2/STD.xsd" xsi:schemaLocation="http://www.openmicroscopy.org/XMLschemas/STD/RC2/STD.xsd http://www.openmicroscopy.org/XMLschemas/STD/RC2/STD.xsd">
		<SemanticType Name="MultipleScaleHistograms" AppliesTo="I">
			<Description>
			Normalized (by the maximal value) multiscale histograms (number of bins 3,5,7,9) 
			results in 1x24 vector.
			</Description>
			<Element
				Name="TBins3_Bin00"
				DBLocation="MSCALEHIST.c3BINS_BIN_00"
				DataType="float"/>
			<Element
				Name="TBins3_Bin01"
				DBLocation="MSCALEHIST.c3BINS_BIN_01"
				DataType="float"/>
			<Element
				Name="TBins3_Bin02"
				DBLocation="MSCALEHIST.c3BINS_BIN_02"
				DataType="float"/>
				
			<Element
				Name="TBins5_Bin00"
				DBLocation="MSCALEHIST.c5BINS_BIN_00"
				DataType="float"/>
			<Element
				Name="TBins5_Bin01"
				DBLocation="MSCALEHIST.c5BINS_BIN_01"
				DataType="float"/>
			<Element
				Name="TBins5_Bin02"
				DBLocation="MSCALEHIST.c5BINS_BIN_02"
				DataType="float"/>
			<Element
				Name="TBins5_Bin03"
				DBLocation="MSCALEHIST.c5BINS_BIN_03"
				DataType="float"/>
			<Element
				Name="TBins5_Bin04"
				DBLocation="MSCALEHIST.c5BINS_BIN_04"
				DataType="float"/>
				
			<Element
				Name="TBins7_Bin00"
				DBLocation="MSCALEHIST.c7BINS_BIN_00"
				DataType="float"/>
			<Element
				Name="TBins7_Bin01"
				DBLocation="MSCALEHIST.c7BINS_BIN_01"
				DataType="float"/>
			<Element
				Name="TBins7_Bin02"
				DBLocation="MSCALEHIST.c7BINS_BIN_02"
				DataType="float"/>
			<Element
				Name="TBins7_Bin03"
				DBLocation="MSCALEHIST.c7BINS_BIN_03"
				DataType="float"/>
			<Element
				Name="TBins7_Bin04"
				DBLocation="MSCALEHIST.c7BINS_BIN_04"
				DataType="float"/>
			<Element
				Name="TBins7_Bin05"
				DBLocation="MSCALEHIST.c7BINS_BIN_05"
				DataType="float"/>
			<Element
				Name="TBins7_Bin06"
				DBLocation="MSCALEHIST.c7BINS_BIN_06"
				DataType="float"/>
				
			<Element
				Name="TBins9_Bin00"
				DBLocation="MSCALEHIST.c9BINS_BIN_00"
				DataType="float"/>
			<Element
				Name="TBins9_Bin01"
				DBLocation="MSCALEHIST.c9BINS_BIN_01"
				DataType="float"/>
			<Element
				Name="TBins9_Bin02"
				DBLocation="MSCALEHIST.c9BINS_BIN_02"
