<AnalysisChains xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openmicroscopy.org/XMLschemas/AnalysisModule/IR3/AnalysisChain.xsd">
  <AnalysisChain Name="Pixels Slice Test" Locked="false">
    <Nodes>
		<Node NodeID = "Files"  ProgramName = "Original files"/>
		<Node NodeID = "Import" ProgramName = "Image import"/>
		<Node NodeID = "CreatePixelsSlices"   ProgramName = "Create pixels slices"/>
		<Node NodeID = "CreateChannelSlices"  ProgramName = "Create channel slices"/>
		<Node NodeID = "CreateStackSlices"    ProgramName = "Create stack slices"/>
		<Node NodeID = "CreatePlaneSlices"    ProgramName = "Create plane slices"/>

		<Node NodeID = "PixelsSlice->Pixels"  ProgramName = "PixelsSlice -> Pixels"/>
		<Node NodeID = "PixelsChannelSlice->PixelsSlice" ProgramName = "PixelsChannelSlice -> PixelsSlice"/>
		<Node NodeID = "PixelsStackSlice->PixelsSlice"   ProgramName = "PixelsStackSlice -> PixelsSlice"/>
		<Node NodeID = "PixelsPlaneSlice->PixelsSlice"   ProgramName = "PixelsPlaneSlice -> PixelsSlice"/>
    </Nodes>
    <Links>
		<Link FromNodeID="Files"  FromOutputName="Files"  ToNodeID="Import" ToInputName="Files"/>
		<Link FromNodeID="Import" FromOutputName="Pixels" ToNodeID="CreatePixelsSlices" ToInputName="Pixels"/>
		<Link FromNodeID="CreatePixelsSlices" FromOutputName="Slices" ToNodeID="CreateChannelSlices" ToInputName="Slices"/>
		<Link FromNodeID="CreateChannelSlices" FromOutputName="Channel slices" ToNodeID="PixelsChannelSlice->PixelsSlice" ToInputName="Channel slices"/>
		<Link FromNodeID="PixelsChannelSlice->PixelsSlice" FromOutputName="Slices" ToNodeID="CreateStackSlices" ToInputName="Slices"/>
		<Link FromNodeID="CreateStackSlices" FromOutputName="Stack slices" ToNodeID="PixelsStackSlice->PixelsSlice" ToInputName="Stack slices"/>
		<Link FromNodeID="PixelsStackSlice->PixelsSlice" FromOutputName="Slices" ToNodeID="CreatePlaneSlices" ToInputName="Slices"/>
		<Link FromNodeID="CreatePlaneSlices" FromOutputName="Plane slices" ToNodeID="PixelsPlaneSlice->PixelsSlice" ToInputName="Plane slices"/>
		<Link FromNodeID="PixelsPlaneSlice->PixelsSlice" FromOutputName="Slices" ToNodeID="PixelsSlice->Pixels" ToInputName="Slices"/>
    </Links>
  </AnalysisChain>
</AnalysisChains>

